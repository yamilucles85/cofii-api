{
  "name": "Coffee",
  "plural": "coffees",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "model": {
      "type": "string"
    },
    "avg_rating": {
      "type": "number",
      "required": true
    },
    "altitude": {
      "type": "number",
      "required": true
    },
    "price": {
      "type": "number",
      "required": false
    },
    "roast": {
      "type": "number",
      "required": false
    },
    "catching_score": {
      "type": "number",
      "required": false
    }
  },
  "validations": [],
  "scope": {
    "include": ["brand", "variety"]
  },
  "relations": {
    "variety": {
      "type": "belongsTo",
      "model": "Variety",
      "foreignKey": "varietyId"
    },
    "brand": {
      "type": "belongsTo",
      "model": "Brand",
      "foreignKey": "brandId"
    },
    "reviews": {
      "type": "hasMany",
      "model": "Review",
      "foreignKey": "coffeeId"
    }
  },
  "acls": [],
  "methods": {}
}
